{
  "name": "atsur-registry",
  "version": "1.0.0",
  "private": true,
  "pre-commit": [
    "format:fix",
    "lint",
    "build"
  ],
  "scripts": {
    "prepare": "husky install",
    "sass": "sass --watch public/assets/scss/main.scss:public/assets/css/main.css",
    "pre-commit": "npm run format:fix && npm run lint && npm run build",
    "dev": "next dev",
    "build": "prisma generate && npm run format:fix && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir './src/contracts/types' './src/contracts/*.json'",
    "postinstall": "yarn compile-contract-types",
    "generate-json": "node scripts/jsonGenerator.js",
    "remove-darkmode": "node scripts/removeDarkMode.js && yarn format"
  },
  "dependencies": {
