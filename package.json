{
  "name": "atsur-registry",
  "version": "1.0.0",
  "private": true,
  "pre-commit": [
    "format:fix",
    "lint",
    "build"
  ],
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "npm run format:fix && npm run lint && npm run build",
    "dev": "next dev",
    "build": "npm run format:fix && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir './src/contracts/types' './src/contracts/*.json'",
    "postinstall": "yarn compile-contract-types",
    "generate-json": "node scripts/jsonGenerator.js",
    "remove-darkmode": "node scripts/removeDarkMode.js && yarn format"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.1",
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/onboarding": "^1.0.1",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.146",
    "@mui/material": "^5.14.4",
    "@mui/styles": "^5.14.14",
    "@next/font": "^13.4.13",
    "@samvera/clover-iiif": "^1.14.3",
    "@sendgrid/mail": "^7.7.0",
    "@vercel/analytics": "^1.0.1",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.2.9",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "disqus-react": "^1.1.5",
    "dotenv": "^16.0.3",
    "firebase": "^9.21.0",
    "firebase-admin": "^11.7.0",
    "fuse.js": "^6.6.2",
    "github-slugger": "^2.0.0",
    "google-auth-library": "^8.8.0",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.5.3",
    "next-auth": "^4.23.1",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "pure-react-carousel": "^1.30.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.2",
    "react-icons": "^4.9.0",
    "react-lite-youtube-embed": "^2.3.52",
    "react-paystack": "^4.0.3",
    "react-slick": "^0.29.0",
    "remark-gfm": "^3.0.1",
    "sass": "^1.63.3",
    "swr": "^2.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "autoprefixer": "^10.4.14",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "ethers": "^5.7.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "styled-components": "^6.0.6",
    "tailwind-bootstrap-grid": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "typechain": "^8.1.1",
    "typescript": "^5.1.6"
  }
}
