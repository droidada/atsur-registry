{
  "name": "atsur-registry",
  "version": "1.0.0",
  "private": true,
  "pre-commit": [
    "format:fix",
    "lint",
    "build"
  ],
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "npm run format:fix && npm run lint && npm run build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir './contracts/types' './contracts/*.json'",
    "postinstall": "yarn compile-contract-types"
  },
  "dependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/onboarding": "^1.0.1",
    "@samvera/clover-iiif": "^1.14.3",
    "@sendgrid/mail": "^7.7.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.2.9",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "firebase": "^9.21.0",
    "firebase-admin": "^11.7.0",
    "jsonwebtoken": "^9.0.0",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-paystack": "^4.0.3",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "ethers": "^5.7.2",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  }
}
